            <TableColumn fx:id="tableOri" prefWidth="118.0" text="Orientation" />
        </columns>
      </TableView>
      <Button layoutX="490.0" layoutY="331.0" mnemonicParsing="false" onAction="#Home" text="Home">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
